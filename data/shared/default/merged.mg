      - cis_csc: ["5.1"]
      - pci_dss: ["7.1"]
      - tsc: ["CC6.4"]
    references:
      - 'CCE-33784-0'
    condition: all
    rules:
      - 'r:HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Policies\System -> ConsentPromptBehaviorAdmin -> 2'

  - id: 15550 
